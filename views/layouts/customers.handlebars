                          </h1> User - DayBreak Retail Management </h1>

 <div id="home">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h1>{{msg}}</h1>
        <h2>Customers</h2>
        <form class="clearfix mb-4" action="POST">
          <div class="form-group">
            <label for="example-text">Customer Information</label>
            <input type="text" id="example-text" class="form-control" aria-describedby="example-text" placeholder="Enter Example Text">
          </div>
          <div class="form-group">
            
          
        </form>
       <h2> Customers Information </h2>
                
                <label for="id"><i class="fas fa-portrait"></i> ID </label>
                <input type="text" id="id" name="id" placeholder="?">

                <label for="fname"><i class="fas fa-file-signature"></i>First Name</label>
                <input type="text" id="name" name="firstname" placeholder="John">

                <label for="lname"><i class="fas fa-file-signature"></i>Last Name</label>
                <input type="text" id="name" name="lastname" placeholder="Doe">

                <label for="email"><i class="fas fa-envelope-square"></i>Email</label>
                <input type="text" id="email" name="email" placeholder="john@example.com">

                    </div>
                    
                <button id="submit" class="btn btn-primary float-right">Submit</button>
      </div>
   </div>   
        <ul id="example-list" class="list-group">
          {{#each examples}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="example/{{this.id}}">{{this.text}}</a>
            <button class="btn btn-danger float-right delete">ï½˜</button>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>

